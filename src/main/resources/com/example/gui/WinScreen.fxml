<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.WinScreen">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="209.0" layoutY="128.0" prefHeight="244.0" prefWidth="382.0" spacing="75.0">
         <children>
            <Label fx:id="winlabel" alignment="TOP_CENTER" prefWidth="200.0" text="You Win!">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="25.0">
               <children>
                  <Button fx:id="playButton" alignment="CENTER" mnemonicParsing="false" onAction="#playAgain" prefHeight="75.0" prefWidth="150.0" text="Play Again">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="QuitButton" mnemonicParsing="false" onAction="#quit" prefHeight="75.0" prefWidth="150.0" text="Quit">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
